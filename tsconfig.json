{
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "Snabbdom.createElement",
    "module": "commonjs",
    "target": "es6",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "DOM",
      "ES6"
    ],
    "typeRoots": [
      "node_modules/@types",
      "node_modules/web-ext-types"
    ]
  },
  "exclude": [
    "public"
  ]
}