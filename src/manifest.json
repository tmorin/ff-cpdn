{
  "manifest_version": 2,
  "name": "Clear private data now!",
  "version": "1.7.0",
  "description": "Clear private data related to the current tab.",
  "icons": {
    "24": "icons/cpdn-24x24.png",
    "48": "icons/cpdn-48x48.png",
    "128": "icons/cpdn-128x128.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "contextualIdentities",
    "cookies",
    "history",
    "tabs",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "24": "icons/cpdn-24x24.png",
      "48": "icons/cpdn-48x48.png",
      "128": "icons/cpdn-128x128.png"
    },
    "default_title": "Clear private data now!",
    "default_popup": "popup.html"
  },
  "applications": {
    "gecko": {
      "id": "{7a07e802-6785-4e48-a29e-b81bdd5efbd3}"
    }
  }
}