{
  "manifest_version": 2,
  "name": "Clear private data now!",
  "version": "1.1.0",
  "description": "Clear private data related to the current tab.",
  "icons": {
    "24": "icons/cpdn-24x24.png",
    "48": "icons/cpdn-48x48.png",
    "128": "icons/cpdn-128x128.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "cookies",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "24": "buttons/cpdn-24x24.png",
      "48": "buttons/cpdn-48x48.png",
      "128": "buttons/cpdn-128x128.png"
    },
    "default_title": "Clear private data now!"
  }
}